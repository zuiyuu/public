        # 单选框设定
        Radiobutton(frame,variable=r_value, text='新的信息提取处理。              请点击信息提取按钮     ', font=('宋体', 11), value=1,command=self.callRadiobutton1).grid(row=rowCount, column=0, columnspan=3, sticky=W)  #设置对齐方位，东E南S西W北N
        rowCount+=1
        Radiobutton(frame,variable=r_value, text='保留已转换完PDF文件，继续处理。  请点击信息提取按钮     ', font=('宋体', 11), value=2,command=self.callRadiobutton2).grid(row=rowCount, column=0, columnspan=3, sticky=W)  #设置对齐方位，东E南S西W北N
        rowCount+=1
